varnishtest "empty object"

shell {
vclpp >actual.vcl <<EOF
vmod.constructor empty { }
EOF
}

shell {
vclpp >expected.vcl <<EOF
sub vcl_init {
	new empty = vmod.constructor();
}
EOF
}

shell {diff -u expected.vcl actual.vcl}
